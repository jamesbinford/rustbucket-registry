# Django settings
SECRET_KEY=your-secret-key-here-generate-with-django
DEBUG=False
ALLOWED_HOSTS=yourdomain.com,www.yourdomain.com,localhost

# Database settings
DB_NAME=rustbucket_registry
DB_USER=rustbucket_user
DB_PASSWORD=change-this-strong-password
DB_ROOT_PASSWORD=change-this-root-password
DB_HOST=localhost
DB_PORT=3306

# AWS S3 settings (for log storage)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_S3_REGION=us-east-1
AWS_S3_BUCKET_NAME=rustbucket-registry-logs

# Email settings (for notifications)
EMAIL_BACKEND=django.core.mail.backends.smtp.EmailBackend
EMAIL_HOST=smtp.gmail.com
EMAIL_PORT=587
EMAIL_USE_TLS=True
EMAIL_HOST_USER=your-email@gmail.com
EMAIL_HOST_PASSWORD=your-app-password
DEFAULT_FROM_EMAIL=rustbucket-registry@yourdomain.com

# Scheduler settings
RUN_SCHEDULER=true

# EC2 Deployment settings
EC2_DEFAULT_INSTANCE_TYPE=t3.micro
EC2_DEFAULT_REGION=us-east-1
EC2_VPC_ID=
EC2_SUBNET_ID=
EC2_SECURITY_GROUP_ID=
EC2_KEY_NAME=
EC2_IAM_INSTANCE_PROFILE=
REGISTRY_BASE_URL=https://your-registry-domain.com

# RustBucket honeypot configuration
# Docker image for deployed honeypots
RUSTBUCKET_DOCKER_IMAGE=jamesbinford/rustbucket:latest

# LLM API keys (set at least one for LLM-powered honeypot responses)
# RustBucket supports OpenAI, Claude (Anthropic), Gemini (Google), and Ollama
RUSTBUCKET_OPENAI_API_KEY=sk-your-openai-key
RUSTBUCKET_ANTHROPIC_API_KEY=
RUSTBUCKET_GOOGLE_API_KEY=